        "gatsby-image": "^2.4.16",
        "gatsby-plugin-postcss": "^2.3.11",
        "gatsby-plugin-react-head": "^1.1.0",
        "gatsby-source-sanity": "^6.0.3",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-head": "^3.3.0",
        "react-map-gl": "^5.2.7"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@storybook/addon-actions": "^5.3.19",
